        {
          "value": "gallery_grid",
          "label": "t:sections.main_product.settings.desktop_media_layout.options.option_gallery_grid.label"
        }
      ]
    },
    {
      "type": "select",
      "id": "width_on_large_screen",
      "label": "t:sections.main_product.settings.width_on_large_screen.label",
      "default": "product__media-wrapper--full",
      "options": [
        {
          "value": "product__media-wrapper--full",
          "label": "t:sections.main_product.settings.wide.label"
        },
        {
          "value": "product__media-wrapper--compact",
          "label": "t:sections.main_product.settings.compact.label"
        }
      ]
    },
    {
      "type": "checkbox",
      "id": "enable_pagination_mobile",
      "label": "t:general.settings.enable_pagination_mobile.label",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "enable_thumbnail_mobile",
      "label": "t:sections.main_product.settings.enable_media_thumbnail_mobile.label",
      "default": true
    },
    {
