        },
        {
          "group": "t:general.settings.fit.label",
          "value": "2/3 fit",
          "label": "t:general.settings.image_ratio.options.option_portrait.label"
        },
        {
          "group": "t:general.settings.fit.label",
          "value": "3/2 fit",
          "label": "t:general.settings.image_ratio.options.option_landscape.label"
        }
      ]
    },
    {
      "type": "checkbox",
      "id": "enable_media_zoom",
      "label": "t:sections.main_product.settings.desktop_media_layout.enable_media_zoom.label",
